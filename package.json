{
  "name": "tasks",
  "version": "0.0.1",
  "description": "A template for projects used in Makimo.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config config/webpack.local.js --mode development",
    "build-dist": "webpack --config config/webpack.dist.js --mode production --progress --colors && sed -i -e 's?'`pwd`'?/home/sites/vhosts/tasks.makimo.pl?g' webpack-stats.dist.json",
    "watch-assets": "webpack --config config/webpack.local.js --watch --mode development",
    "watch-python": "./manage.py runserver 0.0.0.0:8000",
    "dev": "concurrently npm:watch-*"
  },
  "author": "Makimo",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^5.3.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.4.2",
    "rimraf": "^2.7.1",
    "sass": "^1.54.0",
    "sass-loader": "^10",
    "style-loader": "^0.23.0",
    "vue-loader": "^15.10.0",
    "vue-template-compiler": "^2.7.8",
    "webpack": "^5.74.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.1.4",
    "webpack-notifier": "^1.15.0"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bootstrap": "^4.6.2",
    "deep-equal": "^1.1.1",
    "jquery": "^3.6.0",
    "moment": "^2.29.4",
    "popper.js": "^1.16.1",
    "vue": "^2.7.8",
    "vue-context": "^5.2.0",
    "vue-global-events": "^1.2.1",
    "vue-router": "^3.5.4",
    "vuex": "^3.6.2"
  }
}
